{
  "expo": {
    "name": "Map Meter",
    "slug": "map-meter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A1E26"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.bymoxb.mapmeter",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCZaiPHwnZIC4mMKhMNNODO_tcD5A68HOA"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "userInterfaceStyle": "automatic",
    "description": "A react native app, to measure distances, perimeters, and areas",
    "githubUrl": "https://github.com/bymoxb/map-meter"
  }
}
